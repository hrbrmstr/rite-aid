<script>
if(window.location.pathname=='/signup-signin' && (window.location.hash=='' || window.location.hash=='#login')){
        window.location.href=window.location.origin+"/login";    
      }else if(window.location.pathname=='/signup-signin' &&  window.location.hash=='#signup'){
        window.location.href=window.location.origin+"/signup";    
      } else if(window.location.pathname=='/signup-signin' &&  window.location.hash.includes('#resetPassword')){
		var queryParams= window.location.hash.split('#resetPassword')[1];
        window.location.href=window.location.origin+ "/resetpassword" + queryParams;    
      }
</script>
